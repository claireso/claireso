{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "@components": "./components",
          "@hooks": "./hooks",
          "@hoc": "./hoc"
        }
      }
    ]
  ]
}